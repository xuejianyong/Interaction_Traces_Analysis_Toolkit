import java.util.ArrayList;
import java.util.List;

import dl.Interaction;

public class Patterns {

	public Interaction enactedInteraction;
	public List<Interaction> anticipations = new ArrayList<Interaction>();
	public int weight;
	public Interaction getEnactedInteraction() {
		return enactedInteraction;
	}
	public void setEnactedInteraction(Interaction enactedInteraction) {
		this.enactedInteraction = enactedInteraction;
	}
	public List<Interaction> getAnticipations() {
		return anticipations;
	}
	public void setAnticipations(List<Interaction> anticipations) {
		this.anticipations = anticipations;
	}
	public int getWeight() {
		return weight;
	}
	public void setWeight(int weight) {
		this.weight = weight;
	}
	
	
	public void incrementWeight() {
		this.weight++;
	}
	
	
}
